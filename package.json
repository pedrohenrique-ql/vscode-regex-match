{
  "name": "regex-match",
  "displayName": "Regex Match",
  "description": "",
  "version": "0.0.0",
  "engines": {
    "node": ">= 20.11.0 <21",
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "regex-match.openRegexTestWindow",
        "title": "Regex Match: Open Regex Test Window"
      }
    ],
    "keybindings": {
      "command": "regex-match.openRegexTestWindow",
      "key": "ctrl+alt+x",
      "mac": "cmd+alt+x"
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsup",
    "pre:test": "pnpm run compile",
    "test:vsc": "pnpm pre:test && vscode-test",
    "test:vi": "vitest",
    "lint": "eslint --ext 'ts' --cache --no-error-on-unmatched-pattern --max-warnings 0 --fix",
    "style": "prettier --log-level warn --ignore-unknown --no-error-on-unmatched-pattern --cache",
    "style:check": "pnpm style --check",
    "style:format": "pnpm style --write",
    "types:check": "tsc --noEmit",
    "pre:commit": "lint-staged",
    "pre:push": "concurrently --names style,lint,types 'pnpm style:check .' 'pnpm lint .' 'pnpm types:check'",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/tmp": "^0.2.6",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.2.0",
    "glob": "^7.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "prettier-plugin-sh": "^0.14.0",
    "tmp": "^0.2.3",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
